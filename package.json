{
  "name": "astro-preact-zustand-starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "test": "vitest --coverage",
    "build": "astro build",
    "build:secure": "astro build && -c jscrambler -o dist-protected",
